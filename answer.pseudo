#include <iostream>
#include <stdlib.h>

int range_random(int low, int high){
	if(high >= low){
		return (random() % (high - low + 1)) + low; // single thr only
	}
	return low;
}

void print_name_rand(const std::string& name){
	std::cout << name << " " << range_random(6, 15) << "\n";
}

